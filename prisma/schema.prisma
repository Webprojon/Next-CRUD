generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Tasks {
  id          String  @id @default(cuid())
  member      String
  task        String
  priority    String
  iscompleted Boolean
}
